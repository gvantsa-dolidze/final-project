{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "jsx": "react-jsx", // Change to "react" if using an older version
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }